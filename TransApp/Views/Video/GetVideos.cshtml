@model PagedList.IPagedList<TransApp.Models.Video>
@using PagedList.Mvc;

@{
    ViewBag.Title = "GetVideos";
}

@{Html.RenderPartial("VideoPartialView");}

Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) of @Model.PageCount

@Html.PagedListPager(Model, page => Url.Action("GetVideos", new { page, sortOrder = ViewBag.CurrentSort }))



